class Solution {
    public int search(int[] nums, int target) {
       int start=0;
        int end=nums.length-1;
        int mid;
        while(start<=end){
           
            if(nums[start]!=target)start++;
            else return start;
            if(nums[end]!=target)end--;
            else return end;
            
        }
        return -1;
    }
}
