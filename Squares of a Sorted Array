boolean sort;
        for(int i=0 ; i<arr.length ; i++){
            sort =false;
            for(int j=1 ; j<arr.length-i ;j++){
                if(arr[j]<arr[j-1]){
                    int temp =arr[j-1];
                    arr[j-1]=arr[j];
                    arr[j]=temp;
                    sort =true;
                }
                
            }
            if(!sort){
                break;
            }
        }
