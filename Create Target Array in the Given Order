class Solution {
    public int[] createTargetArray(int[] nums, int[] index) {
        int[] arr=new int[nums.length];
      ArrayList<Integer> list =new ArrayList<Integer>();
        for(int i =0; i<nums.length;i++){
            int n=index[i];
            list.add(n , nums[i]);
             
            
        }int k=0;
        for(int i:list){
            arr[k]=list.get(k++);
        }
        return arr;
        


    }
}
