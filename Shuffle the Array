class Solution {
    public int[] shuffle(int[] nums, int n) {
        int[] arr = new int[nums.length];
        int j =0;
        int num = nums.length;
        for(int i = 0; i<num ; i++){
            
             arr[i]=nums[j];
            j++;
            i++;
        }
        int k=n; 
        for(int i =1; i<num ;++i){
           
             arr[i]=nums[k];
            k++;
            i++;
        }
        
        
        
        return arr;
    }
}
